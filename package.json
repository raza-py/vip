{
  "name": "vless-proxy-worker",
  "version": "1.0.0",
  "description": "A modern, performant, and reliable VLESS proxy worker for Cloudflare.",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "npm run build:wasm && wrangler dev",
    "preflight": "node ./scripts/preflight.js",
    "deploy": "npm run preflight && npm run build:full && wrangler deploy",
    "deploy:prod": "npm run preflight && npm run build:full && npx wrangler deploy --name vless-proxy-worker",
    "build:wasm": "wasm-pack build src/rust --target web",
    "build:bundle": "npx wrangler deploy --dry-run --outdir dist --name vless-proxy-worker",
    "obfuscate": "node ./scripts/obfuscate.js",
    "build:full": "npm run build:wasm && npm run build:bundle && npm run obfuscate",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "drizzle-orm": "^0.30.10",
    "hono": "^4.3.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240403.0",
    "better-sqlite3": "^9.6.0",
    "biome": "^0.3.3",
    "drizzle-kit": "^0.21.4",
    "javascript-obfuscator": "^4.1.0",
    "miniflare": "^4.20251118.0",
    "vitest": "^1.6.1",
    "vitest-environment-miniflare": "^2.14.4",
    "wrangler": "^4.49.1"
  }
}
