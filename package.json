{
  "name": "vless-proxy-worker",
  "version": "1.0.0",
  "description": "A modern, performant, and reliable VLESS proxy worker for Cloudflare.",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "npm run build:wasm && wrangler dev",
    "deploy": "npm run build:wasm && wrangler deploy",
    "build:wasm": "wasm-pack build src/rust --target web",
    "test": "vitest",
    "lint": "biome lint .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "drizzle-orm": "^0.30.10",
    "hono": "^4.3.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240403.0",
    "better-sqlite3": "^9.6.0",
    "biome": "^1.6.0",
    "drizzle-kit": "^0.21.4",
    "javascript-obfuscator": "^4.1.0",
    "vitest": "^1.6.0",
    "wrangler": "^3.57.0"
  }
}
